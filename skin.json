{
	"name": "Eithalica Wiki",
	"version": "1.0.0",
	"author": [
		"..."
	],
	"url": "https://eithalica.world/lore/w/index.php",
	"descriptionmsg": "eithalica-skin-desc",
	"namemsg": "skinname-eithalica",
	"license-name": "CC0-1.0",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.38.0"
	},
	"ValidSkinNames": {
		"eithalica": {
			"class": "SkinMustache",
			"args": [
				{
					"name": "eithalica",
					"responsive": true,
					"messages": [
						"eithalica-no-categories",
						"sitetitle",
						"search",
						"otherlanguages",
						"tagline",
						"navigation-heading"
					],
					"styles": [
						"mediawiki.ui.icon",
						"mediawiki.ui.button",
						"skins.eithalica.styles"
					],
					"scripts": [
						"skins.eithalica"
					]
				}
			]
		}
	},
	"MessagesDirs": {
		"Eithalica": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.eithalica.styles": {
			"class": "ResourceLoaderSkinModule",
			"features": {
				"normalize": true,
				"elements": true,
				"content-tables": true,
				"content-links": false,
				"content-media": false,
				"content-links-external": false,
				"interface-message-box": true,
				"interface-category": false,
				"toc": true
			},
			"targets": [
				"desktop",
				"mobile"
			],
			"styles": [
				"resources/skin.less"
			]
		},
		"skins.eithalica": {
			"targets": [
				"desktop",
				"mobile"
			],
			"packageFiles": [
				"resources/skin.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Eithalica"
	},
	"ResourceModuleSkinStyles": {
		"eithalica": {
			"+ext.echo.styles.badge": "resources/extensions/echo.styles.badge.less"
		}
	},
	"manifest_version": 2
}
